{
  "name": "blog-react",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "client:build": "node scripts/build.js",
    "server:build": "webpack --env.NODE_ENV=production",
    "local:debug": "concurrently --kill-others \"webpack -w --env.NODE_ENV=development\" \"nodemon \"build/server.js\"\"",
    "local:start": "concurrently --kill-others \"webpack -w --env.NODE_ENV=development\" \"nodemon \"build/server.js\"\"",
    "posts:build": "node mock/scripts/build-posts.js ./mock/post.config.json",
    "posts:manage": "node mock/scripts/manage-posts.js",
    "posts:syndicate": "node mock/scripts/syndicate-posts.js ./mock/syndicate.config.json",
    "scripts:build": "webpack --config mock/webpack.config.js",
    "heroku-postbuild": "npm run posts:syndicate && npm run client:build && npm run server:build",
    "start": "node build/server.js"
  },
  "author": "Josh Wang",
  "license": "MIT",
  "description": "an open source blog implemented in React",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
